{
  "id": "laoxin.app.sub",
  "name": "laoxin 脚本订阅",
  "description": "该订阅兼容BoxJs",
  "author": "@laoxinH",
  "icon": "https://avatars.githubusercontent.com/u/53307507?s=400&u=d2e77cb0686d63791da9712eff168a068e96ab88&v=4",
  "repo": "https://github.com/laoxinH/MyScript",
  "apps": [
    {
      "id": "我在校园健康签到",
      "name": "日在校园签到",
      "keys": [
        "Cookie_wzxy"
      ],
      "scripts": [
        {
          "name": "我在校园健康打卡",
          "script": "https://raw.githubusercontent.com/laoxinH/MyScript/main/Quantumult/wozaixiaoyuan/wzxy_health.js"
        }
      ],
      "settings": [
        {
          "id": "wzxy_autoLocation",
          "name": "根据经纬度获取位置信息",
          "val": true,
          "type": "boolean",
          "desc": "开启该设置后,只需要填写答案,经度,纬度三个选项,其余选项由脚本获取,若不开启均需手动填写"
        },
        {
          "id": "wzxy_answers",
          "name": "答案",
          "val": "",
          "type": "text",
          "desc": "答案格式:[\"0\",\"0\",\"0\",\"2\",\"3\",\"2\"]"
        },
        {
          "id": "wzxy_longitude",
          "name": "经度",
          "val": "",
          "type": "text",
          "desc": "示列格式:31.666567"
        },
        {
          "id": "wzxy_latitude",
          "name": "纬度",
          "val": "",
          "type": "text",
          "desc": "示列格式:106.769248"
        },
        {
          "id": "wzxy_country",
          "name": "国家",
          "val": "中国",
          "type": "text",
          "desc": "示列格式:中国(默认中国)"
        },
        {
          "id": "wzxy_city",
          "name": "城市",
          "val": "",
          "type": "text",
          "desc": "示列格式:北京市"
        },
        {
          "id": "wzxy_district",
          "name": "区(县)",
          "val": "",
          "type": "text",
          "desc": "示列格式:高新区(安岳县)"
        },
        {
          "id": "wzxy_province",
          "name": "省",
          "val": "",
          "type": "text",
          "desc": "示列格式:河南省"
        },
        {
          "id": "wzxy_township",
          "name": "街道(镇)",
          "val": "",
          "type": "text",
          "desc": "示列格式:东城街道(大塘镇)"
        },
        {
          "id": "wzxy_street",
          "name": "路",
          "val": "",
          "type": "text",
          "desc": "示列格式:某路(如凤阳街)"
        },
        {
          "id": "wzxy_areacode",
          "name": "地区编码",
          "val": "",
          "type": "text",
          "desc": "示列格式:某路(如凤阳路)"
        }
      ],
      "author": "@laoxinH",
      "repo": "https://github.com/laoxinH/MyScript",
      "icons": [
        "https://github.com/laoxinH/MyScript/blob/main/Quantumult/wozaixiaoyuan/icon.jpg?raw=true",
        "https://github.com/laoxinH/MyScript/blob/main/Quantumult/wozaixiaoyuan/icon.jpg?raw=true"
      ]
    }
  ]
}
