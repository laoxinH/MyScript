{
  "id": "laoxin.app.sub",
  "name": "laoxin 脚本订阅",
  "description": "BoxJS配置订阅",
  "author": "@laoxinH",
  "icon": "https://lk.laoxinhxx.workers.dev/https://avatars.githubusercontent.com/u/53307507?s=400&u=d2e77cb0686d63791da9712eff168a068e96ab88&v=4",
  "repo": "https://github.com/laoxinH/MyScript",
  "apps": [
    {
      "id": "我在校园健康签到",
      "name": "日在校园签到",
      "keys": [
        "wzxy_cookie",
        "wzxx_UA"
      ],
      "scripts": [
        {
          "name": "我在校园健康打卡",
          "script": "https://lk.laoxinhxx.workers.dev/https://raw.githubusercontent.com/laoxinH/MyScript/main/Quantumult/wozaixiaoyuan/wzxy_health.js"
        }
      ],
      "settings": [
        {
          "id": "wzxy_autoLocation",
          "name": "根据经纬度获取位置信息",
          "val": true,
          "type": "boolean",
          "desc": "开启该设置后,只需要填写答案,经度,纬度三个选项,其余选项由脚本获取,若不开启均需手动填写"
        },
        {
          "id": "wzxy_autoLogin",
          "name": "通过账号密码登录",
          "val": false,
          "type": "boolean",
          "desc": "开启该设置后,将不再通过脚本获取登录信息,需要填写账号,密码两项"
        },{
          "id": "wzxy_username",
          "name": "账号",
          "val": "",
          "type": "text",
          "desc": "账号填写处(一般为电话号码)开启\"通过账号密码登录\"需要填写"
        },{
          "id": "wzxy_password",
          "name": "密码",
          "val": "",
          "type": "text",
          "desc": "账号填写处(一般为电话号码)开启\"通过账号密码登录\"需要填写"
        },
        {
          "id": "wzxy_answers",
          "name": "答案",
          "val": "",
          "type": "text",
          "desc": "答案格式:[\"0\",\"0\",\"0\",\"2\",\"3\",\"无其他情况\"](0对应第一个选项,1对应第二个选项,以此类推)"
        },
        {
          "id": "wzxy_longitude",
          "name": "经度",
          "val": "",
          "type": "text",
          "desc": "示列格式:31.666567"
        },
        {
          "id": "wzxy_latitude",
          "name": "纬度",
          "val": "",
          "type": "text",
          "desc": "示列格式:106.769248"
        }
      ],
      "author": "@laoxinH",
      "repo": "https://github.com/laoxinH/MyScript",
      "icons": [
        "https://lk.laoxinhxx.workers.dev/https://github.com/laoxinH/MyScript/blob/main/Quantumult/wozaixiaoyuan/icon.jpg?raw=true",
        "https://lk.laoxinhxx.workers.dev/https://github.com/laoxinH/MyScript/blob/main/Quantumult/wozaixiaoyuan/icon.jpg?raw=true"
      ]
    },

